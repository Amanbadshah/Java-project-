package com.springcore.ci;

public class Person {
	private String name;
	private int id;
	private String Certi certi;

public Person(String name,int id,String certi) {
	this.name=name;
	this.id=id;
	this.certi=certi;}
public String toString() {
	return this.name+ "  : " +this.id;"{"+this.certi.name+"}"
	
 }
}
